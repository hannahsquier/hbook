<%= render 'cover' %>

<%= render 'subnav' %>

<!-- photos -->
<main class="row">
  <section class="col-xs-offset-1 col-xs-10 content">
    <h2>Photos<a href="#"><button class="btn btn-primary btn-photo pull-right" id="add-photo">Add Photo</button></a></h2>

    <section class="row">
      <% 13.times do %>
      <article id="photo" class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
        <p>
          <a class="hovertext" href="#" title="Uploaded January 1, 2016">
            <%= image_tag "icon_photo_small.jpg", class: "photo" %>
          </a>
        </p>
      </article>
      <% end %>

    </section>
  </section>

</main>

<%= render 'footer' %>